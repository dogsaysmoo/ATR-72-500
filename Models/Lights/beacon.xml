<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList>

	<path>rednav.ac</path>

	<animation>
		<name>On/off</name>
		<type>select</type>
		<object-name>Light</object-name>
		<condition>
			<greater-than>
				<property>systems/electrical/outputs/beacon</property>
				<value>5</value>
			</greater-than>
		</condition>
	</animation>

	<animation>
		<name>Billboard</name>
		<type>billboard</type>
		<object-name>Light</object-name>
	</animation>

</PropertyList>
